    "babel-loader": "^10.0.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.9",
    "open-cli": "^8.0.0",
    "ts-node": "^10.9.2",
    "vitest": "^3.1.2",
    "webpack": "^5.98.0",
